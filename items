#include<stdio.h>
void main()
{
int x;
printf("1.chicken wings,RS 400\n2.Manchuria,Rs 115\n3.Noodles,Rs 130\n4.French Fries,Rs 99\n5.Fried Rice,Rs 300\n");
scanf("%d",&x);
switch(x)
{
case 1:
{
printf("Food item- Chicken Wings\nPrice -Rs 400");
break;
}
case 2:
{
printf("Food item- Manchuria\nPrice -Rs 115");
break;
}
case 3:
{
printf("Food item- Noodles\nPrice -Rs 130");
break;
}
case 4:
{
printf("Food item- French Fries\nPrice -Rs 99");
break;
}
case 5:
{
printf("Food item- Fried Rice\nPrice -Rs 300");
break;
}
default :
{
printf("Item not available");
}
}
}
